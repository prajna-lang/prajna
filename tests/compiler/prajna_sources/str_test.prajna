@test
func testMain() {
    var tmp = str::create(2);
    tmp[0] = 'H';
    tmp[1] = 'i';
    debug::assert(tmp[1] == 'i');
    tmp.print();
    debug::assert(tmp.size == 2);
    tmp.append('!');
    tmp.append('\n');
    tmp.print();
    debug::assert(tmp.size == 4);

    ("Hello" + " World!\n").print();

    var str1 = str::create(2);
    str1 = "苹果";
    var str2 = str::create(2);
    str2 = "华为";
    //*str1.tostr().print();
    if (str1 != str2){
        ("NotEqual is true!\n").print();
    }else{
        ("NotEqual is false!\n").print();
    }
    if (str1 == str2){
        ("Equal is true!\n").print();
    }else{
        ("Equal is false!\n").print();
    }

}
