@test
func TestVariant1() {
    var variant2: Variant<i64>;
    variant2.Set<0>(100);
    test::Assert(variant2.Is<0>());
    test::Assert(variant2.Get<0>() == 100);
}

@test
func TestVariant2() {
    var variant2: Variant<i64, f64>;
    variant2.Set<0>(100);
    test::Assert(variant2.Is<0>());
    test::Assert(variant2.Get<0>() == 100);

    variant2.Set<1>(2.718f64);
    test::Assert(variant2.Is<1>());
    test::Assert(variant2.Get<1>() == 2.718f64);
}

@test
func TestVariant3() {
    var variant3: Variant<i64, f64, i128>;
    variant3.Set<0>(100);
    test::Assert(variant3.Is<0>());
    test::Assert(variant3.Get<0>() == 100);

    variant3.Set<1>(2.718f64);
    test::Assert(variant3.Is<1>());
    test::Assert(variant3.Get<1>() == 2.718f64);

    variant3.Set<2>(100i128);
    test::Assert(variant3.Is<2>());
    test::Assert(variant3.Get<2>() == 100i128);
}
