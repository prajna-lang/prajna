@test
func TestSortArray() {
   var arr: Array<i64, 6> = [64, 13, 666, 25, 92, 88];

   arr.Sort();

   test::Assert(arr[0] == 13);
   test::Assert(arr[1] == 25);
   test::Assert(arr[2] == 64);
   test::Assert(arr[3] == 88);
   test::Assert(arr[4] == 92);
   test::Assert(arr[5] == 666);

   arr.SortReverse();

   test::Assert(arr[0] == 666);
   test::Assert(arr[1] == 92);
   test::Assert(arr[2] == 88);
   test::Assert(arr[3] == 64);
   test::Assert(arr[4] == 25);
   test::Assert(arr[5] == 13);
}

@test
