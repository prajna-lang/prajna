
struct People{
    name: String;
    _age: i64;
}

interface AgeProperty {
    func Get()->i64;
    func Set(v: i64);
}

implement AgeProperty for People{
    func Get()->i64{
        return this._age;
    }

    func Set(v: i64){
        this._age = v;
    }
}

@test
func TestProperty(){
    var people = People("Mike", 18);
    test::Assert(people._age == 18);
    // 我们可以像字段一样使用属性
    test::Assert(people.Age == 18);
    people.Age = 22;
    test::Assert(people._age == 22);
    test::Assert(people.Age == 22);

    people.Age;
}
