@test
func TestVariant2() {
    var variant = Variant2<i8, i16>::CreateFirst(111i8);
    test::Assert(variant.GetFirst() == 111i8);

    variant.EmplaceSecond(222i16);
    test::Assert(variant.GetSecond() == 222i16);
}

