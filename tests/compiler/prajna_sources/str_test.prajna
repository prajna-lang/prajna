@test
func TestMain() {
    var tmp = String::Create(2);
    tmp[0] = 'H';
    tmp[1] = 'i';
    test::Assert(tmp[1] == 'i');
    tmp.Print();
    test::Assert(tmp.size == 2);
    tmp.Append('!');
    tmp.Append('\n');
    tmp.Print();
    test::Assert(tmp.size == 4);

    ("Hello" + " World!\n").Print();

    var str1 = String::Create(2);
    var str2 = String::Create(2);
    str1 = "こんにちは世界";
    str2 = "こんにちは世界";
    //str1.ToString().Print();
    test::Assert(str1 == str2);

    str1 = "hello world";
    str2 = "a@#$%^&*()_-+=[]{}|;:',.<>/?b";
    test::Assert(str1 != str2);

    str1 = "";
    str2 = "";
    test::Assert(str1 == str2);
}
