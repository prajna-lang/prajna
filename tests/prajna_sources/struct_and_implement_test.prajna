
struct People{
    name: String;
    age: i64;
}

implement People{
    func sayHello(){
        "Hello\n".Print();
    }

    func SayHi(){
        this.sayHello();
        ("Hi, My name is " + this.name + ".\n").Print();
    }

    // static注解则为静态成员函数
    @static
    func GetAge(people: People)->i64{
        return people.age;
    }
}

struct NestedMemberFunction {}

implement NestedMemberFunction {
    func Test() {
        this.Test();
    }
}

@test
func TestStructAndImplement(){
    var mike: People;
    mike.name = "Mike";
    mike.age = 18;
    // "."索引成员函数
    mike.SayHi();
    // 注意域成员都是通过::来索引的
    test::Assert(People::GetAge(mike) == 18);
}
