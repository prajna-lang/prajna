@test
func Test1() {
    var tmp: Hash<i64>;
    tmp(3).PrintLine();

    var tmp1: Hash<i32>;
    tmp1(2i32).PrintLine();

    var tmp2: Hash<f64>;
    tmp2(4.0f64).PrintLine();

    var tmp3: Hash<f32>;
    tmp3(3.1f32).PrintLine();

    var tmp4: Hash<char>;
    tmp4('b').PrintLine();

    var tmp5: Hash<String>;
    tmp5("hello").PrintLine();

    var tmp6: Hash<bool>;
    tmp6(true).PrintLine();
}


template <T>
struct Tmp {
    value: T;
}

special <i64>
struct Tmp {
    value: i64;
    flag: bool;
}

@test
func Test2() {
    var tmp: Tmp<i64>;
    tmp.flag = true;
    tmp.flag.PrintLine();
}
